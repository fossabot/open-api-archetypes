{
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/lib/**/javascript-client/"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 90,
            "functions": 90,
            "lines": 90,
            "statements": 90
        }
    },
    "testPathIgnorePatterns": [
        "/node_modules/",
        "/lib/**/javascript-client/test"
    ],
    "verbose": true
}
